{
  "name": "app",
  "private": "true",
  "scripts": {
    "dev": "bin/dev",
    "build": "esbuild app/javascript/*.tsx --bundle --sourcemap --outdir=app/assets/builds --public-path=assets",
    "build:css": "sass ./app/assets/stylesheets/application.sass.scss:./app/assets/builds/application.css --no-source-map --load-path=node_modules",
    "check-types": "tsc --project tsconfig.json --noEmit --watch --preserveWatchOutput",
    "rebuild": "npm run build && npm run build:css && npm run tsc && npm run lint",
    "tsc": "tsc --project tsconfig.json",
    "eslint": "eslint app/javascript/**/*.ts[x]",
    "eslint-ci": "node --max-old-space-size=4096 ./node_modules/eslint/bin/eslint app/javascript/**/*.ts[x]",
    "eslint-fix": "eslint --fix --cache app/javascript/**/*.ts[x]"
  },
  "dependencies": {
    "@apollo/client": "^3.7.4",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@hotwired/stimulus": "^3.2.1",
    "@hotwired/turbo-rails": "^7.2.4",
    "@mui/material": "^5.11.5",
    "@rails/ujs": "^7.0.4-1",
    "crypto-js": "^4.1.1",
    "esbuild": "^0.17.0",
    "graphql": "^16.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.6.2",
    "sass": "^1.57.1",
    "typescript": "^4.9.4"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.1.1",
    "@types/node": "^18.11.18",
    "@types/rails__ujs": "^6.0.1",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.32.0",
    "eslint-config-preact": "^1.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-react-data-attr": "^0.0.4",
    "eslint-plugin-react-hooks": "^4.6.0"
  }
}
